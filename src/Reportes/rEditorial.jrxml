	<queryString>
		<![CDATA[SELECT
     editorial.`codEdi`,
     editorial.`nomEdi`,
     editorial.`direccion`,
     editorial.`telefono`,
     editorial.`Correo`,
     pais.`nomPais`
FROM
     `Pais` pais INNER JOIN `Editorial` editorial ON pais.`codPais` = editorial.`pais`
WHERE
     editorial.nomEdi LIKE $P{nombre}
     or pais.nomPais LIKE $P{pais}]]>
	</queryString>
