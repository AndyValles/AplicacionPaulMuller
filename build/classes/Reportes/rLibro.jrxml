	<queryString>
		<![CDATA[select libro.codLib,libro.titulo,autor.nomAut,libro.fechaPublicacion,categoria.nomCat,editorial.nomEdi,libro.edicion,libro.estado from Libro libro 
    Inner join Autor autor on autor.codAut=libro.autor Inner join Categoria categoria on categoria.codCat=libro.categoria
    Inner join Editorial editorial on editorial.codEdi=libro.editorial where 
    libro.codLib like $P{codigo} or  libro.titulo like $P{titulo} or autor.nomAut like $P{Autor} or 
    libro.fechaPublicacion like  $P{fecha} or categoria.nomCat like  $P{categoria} or  editorial.nomEdi like $P{editorial} or libro.edicion like $P{Edicion}
    or libro.estado in ($P{estado1},$P{estado2});]]>
	</queryString>
